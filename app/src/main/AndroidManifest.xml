        <activity
            android:name=".model.thirdpart.AliPayH5Activity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".ShareTargetActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".MapConfigActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".ShareActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".ShareElemActivity"
