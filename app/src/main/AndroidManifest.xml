        <service
            android:name=".xyzrunpackage.RunDeamonService"
            android:enabled="true"
            android:exported="true"
            android:process=":com.runnerfun.deamon"/>

        <!-- new version end -->

        <provider
            android:name=".RecordProvider"
            android:authorities="com.runnerfun.record"
            android:enabled="true"
            android:exported="false"/>

        <activity
            android:name=".CoinDetailActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".UserSettingActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".RecordDetailActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".GuideActivity"
